        "jspm-tsc-update": "^0.1.2",
        "less": "^2.7.1",
        "lodash.assign": "^4.0.6",
        "material-design-icons": "^3.0.1",
        "materialize-css": "^0.98.2",
        "module-name": "0.0.1-security",
        "node-exec-promise": "^0.1.0",
        "normalize.css": "^4.1.1",
        "tslint": "^5.4.3",
        "typescript": "^2.2.1"
    },
    "jspm": {
        "configFiles": {
            "jspm": "system.config.js"
        },
        "configFile": "system.config.js",
        "dependencies": {
            "@angular/forms": "npm:@angular/forms@^4.1.1",
            "@angular/http": "npm:@angular/http@^4.1.1",
            "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^4.1.1",
            "@angular/router": "npm:@angular/router@^4.1.1",
