{
    "name": "angular2-quiz-app",
    "version": "1.8.11",
    "description": "A music quiz in Angular2 using the Spotify API.",
    "main": "server/index.js",
    "scripts": {
        "postinstall": "./node_modules/.bin/jspm install && npm run update-paths && node_modules/.bin/gulp build",
        "heroku-postbuild": "node_modules/.bin/jspm clean && npm prune --production",
        "jspm-tsc-update": "./node_modules/.bin/jspm-tsc-update",
        "update-paths": "./node_modules/.bin/jspm-tsc-update",
        "start": "node server",
        "tsc": "./node_modules/typescript/bin/tsc",
        "tsc:w": "./node_modules/typescript/bin/tsc -w"
    },
    "engines": {
        "node": "6.x.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fabiandev/angular2-quiz-app.git"
    },
    "author": "Fabian Pirklbauer <hi@fabiandev.io>",
    "contributors": [
        "Sascha Zarhuber",
        "Lorenz Krautgartner"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fabiandev/angular2-quiz-app/issues"
    },
    "homepage": "https://github.com/fabiandev/angular2-quiz-app#readme",
    "dependencies": {
        "animate.css": "^3.5.2",
        "body-parser": "^1.15.0",
        "compression": "^1.6.1",
        "debug": "^3.1.0",
        "errorhandler": "^1.4.3",
        "express": "^4.13.4",
        "jspm-resolve": "^2.1.0",
        "method-override": "^2.3.5",
        "newrelic": "^1.28.3",
        "node-env-file": "^0.1.8",
        "node-notifier": "^5.1.2",
        "reflect-metadata": "^0.1.10",
        "spotify-web-api-node": "^2.3.0",
        "yargs": "^8.0.2"
    },
    "devDependencies": {
        "@types/core-js": "^0.9.43",
        "@types/hammerjs": "^2.0.35",
        "@types/iscroll": "^5.2.0",
        "@types/materialize-css": "^0.100.0",
        "del": "^3.0.0",
        "flat-color-icons": "^1.1.0",
        "gulp": "^3.9.1",
        "gulp-clean-css": "^3.5.0",
        "gulp-concat": "^2.6.0",
        "gulp-filter": "^5.0.0",
        "gulp-if": "^2.0.0",
        "gulp-less": "^3.0.5",
        "gulp-preprocess": "^2.0.0",
        "gulp-rename": "^1.2.2",
